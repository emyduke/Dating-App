{
  "name": "DatingApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --active-arch-only",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start-no-cache": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "apk": "cd android && gradlew assembleRelease",
    "generate-splash": "yarn react-native generate-bootsplash src/assets/images/welcome.png --background-color=ffffff --logo-width=200 --assets-path=src/assets --flavor=main",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\""
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "react": "18.0.0",
    "react-native": "0.69.1",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.15.0",
    "tailwind-rn": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "concurrently": "^7.2.2",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "postcss": "^8.4.14",
    "react-test-renderer": "18.0.0",
    "tailwindcss": "^3.1.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
